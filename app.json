{
  "expo": {
    "name": "Vertical Math",
    "slug": "VerticalMath",
    "description": "A visual approach to help kids learn vertical addition, subtraction, and multiplication.",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.erikaybar.verticalmath"
    },
    "android": {
      "package": "com.erikaybar.verticalmath"
    }
  }
}